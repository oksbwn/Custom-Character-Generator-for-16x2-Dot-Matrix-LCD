<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project default="create_run_jar" name="Create Runnable Jar for Project MAX7219 with libraries in sub-folder">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                                        -->
    <target name="create_run_jar">
        <jar destfile="generator.jar">
            <manifest>
                <attribute name="Main-Class" value="newUI"/>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="Source/bin"/>
        </jar>
        <delete dir="generator_lib"/>
        <mkdir dir="generator_lib"/>
    </target>
</project>
